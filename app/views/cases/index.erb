
<section class = "section">
  <div class= "container">
    <div class = "columns is-flex-direction-row is-flex-wrap-wrap">
      <%@cases.each do |p|%>
        <div class = "column">
          <div class = "card">
            <div class ="card-image has-text-centered ">
              <figure class = "">
                <img src="<%= p.image%>" alt=" Product image">
              </figure>
            </div>
            <div class= "card-content">
              <p><%= p.Name%></p>
            </div>
          </div>
        </div>
      <%end%>
    </div>
  </div>
</section>






